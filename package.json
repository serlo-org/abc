{
  "name": "serlo-abc",
  "version": "0.0.0-development",
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "repository": "serlo-org/serlo-abc",
  "scripts": {
    "commit": "git-cz",
    "build": "exp start",
    "build:android": "exp build:android",
    "build:ios": "exp build:ios",
    "build:status": "exp build:status",
    "generate:words": "node scripts/generate-words",
    "postgenerate:words": "yarn format",
    "generate:ids": "node scripts/generate-ids",
    "postgenerate:ids": "yarn format",
    "tsc": "tsc",
    "exp:config": "node scripts/generate-app-config",
    "exp:login": "exp login",
    "exp:publish": "exp publish",
    "start": "react-native-scripts start",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "storybook": "storybook start -p 7007",
    "test": "jest",
    "test:watch": "jest --watch",
    "format":
      "prettier --write \"{{scripts,src,storybook}/**/*,*}.{ts,tsx,js,jsx,json}\"",
    "lint:js": "eslint \"{{scripts,src,storybook}/**/*,*}.{js,jsx}\"",
    "lint:ts": "tslint \"{{scripts,src,storybook}/**/*,*}.{ts,tsx}\"",
    "lint":
      "npm-run-all --print-name --continue-on-error --sequential lint:* tsc",
    "precommit": "lint-staged"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "expo": "^22.0.0",
    "ramda": "^0.25.0",
    "react": "16.0.0-beta.5",
    "react-native": "^0.49.5",
    "react-native-swipe-gestures": "^1.0.2",
    "react-router-native": "^4.2.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.13",
    "@storybook/react-native": "^3.2.13",
    "@types/jest": "^21.1.5",
    "@types/ramda": "^0.25.0",
    "@types/react": "^16.0.19",
    "@types/react-native": "^0.49.5",
    "@types/react-test-renderer": "^16.0.0",
    "commitizen": "^2.9.6",
    "concurrently": "^3.5.0",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "^4.10.0",
    "eslint-config-universe": "^1.0.6",
    "exp": "^45.1.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-expo-ts": "^22.0.1",
    "lint-staged": "^4.3.0",
    "npm-run-all": "^4.1.1",
    "prettier": "^1.7.4",
    "react-dom": "16.0.0-beta.5",
    "react-native-scripts": "^1.6.0",
    "react-native-typescript-transformer": "^1.1.4",
    "react-test-renderer": "^16.0.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "typescript": "^2.6.1"
  },
  "private": true,
  "lint-staged": {
    "{{scripts,src,storybook}/**/*,*}.{ts,tsx,js,jsx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>/__mocks__/fileMock.js"
    },
    "preset": "jest-expo-ts",
    "setupFiles": ["<rootDir>/.enzyme.js"]
  }
}
