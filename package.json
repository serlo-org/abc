{
  "name": "serlo-abc",
  "version": "1.1.1",
  "main": "./node_modules/expo/AppEntry.js",
  "repository": "serlo-org/serlo-abc",
  "scripts": {
    "build": "exp start",
    "build:android": "exp build:android",
    "build:ios": "exp build:ios",
    "build:status": "exp build:status",
    "deploy:development": "npm-run-all --serial exp:config:development exp:login:travis exp:publish",
    "deploy:master": "npm-run-all --serial exp:config:master exp:login:travis exp:publish",
    "generate": "npm-run-all generate:*",
    "generate:ids": "node scripts/generate-ids",
    "generate:sounds": "ts-node scripts/generate-sounds",
    "generate:videos": "ts-node scripts/generate-videos",
    "generate:words-images": "ts-node scripts/generate-words-images",
    "generate:words-sounds": "ts-node scripts/generate-words-sounds",
    "postgenerate": "yarn format",
    "tsc": "tsc",
    "exp:config:development": "ts-node scripts/generate-app-config development",
    "exp:config:master": "ts-node scripts/generate-app-config master",
    "exp:login": "exp login",
    "exp:login:travis": "yarn exp:login --non-interactive --username $EXP_USERNAME --password $EXP_PASSWORD",
    "exp:publish": "exp publish --config app.travis.json",
    "start": "expo start",
    "android": "expo android",
    "ios": "expo ios",
    "devtools": "react-devtools",
    "storybook": "storybook start -p 7007",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write \"{{packages,scripts,src,storybook}/**/*,*}.{ts,tsx,js,jsx,json,md}\"",
    "lint:js": "eslint \"{{packages,scripts,src,storybook}/**/*,*}.{js,jsx}\"",
    "lint:ts": "tslint \"{{packages,scripts,src,storybook}/**/*,*}.{ts,tsx}\"",
    "lint": "npm-run-all --print-name --continue-on-error --sequential lint:* tsc",
    "ncu": "ncu --reject @types/react-native,react,react-dom"
  },
  "dependencies": {
    "chroma-js": "^1.4.0",
    "expo": "^30.0.1",
    "fluent-ffmpeg": "^2.1.2",
    "ramda": "^0.25.0",
    "react": "16.3.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-30.0.0.tar.gz",
    "react-native-swipe-gestures": "^1.0.2",
    "react-router-native": "^4.3.0",
    "sentry-expo": "~1.9.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.11",
    "@storybook/react-native": "^3.4.11",
    "@types/chroma-js": "^1.3.7",
    "@types/fluent-ffmpeg": "^2.1.8",
    "@types/gm": "^1.18.0",
    "@types/jest": "^23.3.2",
    "@types/ramda": "^0.25.38",
    "@types/react-native": "^0.55.28",
    "@types/react-test-renderer": "^16.0.2",
    "@types/storybook__addon-actions": "^3.4.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.6.0",
    "eslint-config-universe": "^1.0.7",
    "expo-cli": "^2.2.0",
    "gm": "^1.23.1",
    "husky": "^1.0.1",
    "jest": "^23.6.0",
    "jest-expo": "^30.0.0",
    "lint-staged": "^7.3.0",
    "npm-check-updates": "^2.14.2",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "react-devtools": "^3.4.0",
    "react-dom": "16.3.1",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "^16.5.2",
    "ts-jest": "^23.10.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.1"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{{packages,scripts,src,storybook}/**/*,*}.{ts,tsx,js,jsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
