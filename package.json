{
  "name": "@serlo/serlo-abc",
  "version": "1.3.0",
  "versionCode": 7,
  "main": "node_modules/expo/AppEntry.js",
  "repository": "serlo/serlo-abc",
  "scripts": {
    "build": "expo start",
    "build:android": "expo build:android --config app.ci.json",
    "build:ios": "expo build:ios --config app.ci.json",
    "build:status": "expo build:status",
    "deploy:development": "npm-run-all --serial expo:config:development expo:publish expo:config:clean",
    "deploy:master": "npm-run-all --serial expo:config:master expo:publish expo:config:clean",
    "deploy:android": "npm-run-all --serial expo:config:master build:android expo:config:clean",
    "deploy:ios": "npm-run-all --serial expo:config:master build:ios expo:config:clean",
    "generate": "npm-run-all generate:*",
    "generate:ids": "node scripts/generate-ids",
    "generate:sounds": "ts-node scripts/generate-sounds",
    "generate:videos": "ts-node scripts/generate-videos",
    "generate:words-images": "ts-node scripts/generate-words-images",
    "generate:words-sounds": "ts-node scripts/generate-words-sounds",
    "postgenerate": "yarn format",
    "tsc": "tsc",
    "expo:config:development": "ts-node scripts/generate-app-config development",
    "expo:config:master": "ts-node scripts/generate-app-config master",
    "expo:config:clean": "rimraf app.ci.json",
    "expo:login": "expo login",
    "expo:publish": "expo publish --config app.ci.json",
    "start": "expo start",
    "android": "expo android",
    "ios": "expo ios",
    "devtools": "react-devtools",
    "storybook": "storybook start -p 7007",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write \"{{packages,scripts,src,storybook}/**/*,*}.{ts,tsx,js,jsx,json,md}\"",
    "lint:js": "eslint \"{{packages,scripts,src,storybook}/**/*,*}.{js,jsx}\"",
    "lint:ts": "tslint \"{{packages,scripts,src,storybook}/**/*,*}.{ts,tsx}\"",
    "lint": "npm-run-all --print-name --continue-on-error --sequential lint:* tsc",
    "changelog": "ts-node scripts/changelog"
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.0",
    "chroma-js": "^2.0.0",
    "expo": "^33.0.0",
    "expo-analytics": "^1.0.0",
    "expo-asset": "^5.0.0",
    "expo-av": "~5.0.0",
    "expo-constants": "~5.0.0",
    "expo-font": "~5.0.0",
    "expo-permissions": "~5.0.0",
    "expo-web-browser": "~5.0.0",
    "fluent-ffmpeg": "^2.0.0",
    "ramda": "^0.26.0",
    "react": "16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-svg": "^9.0.0",
    "react-native-swipe-gestures": "^1.0.0",
    "react-router-native": "^4.0.0",
    "sentry-expo": "~1.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@splish-me/changelog": "^0.1.0",
    "@storybook/addon-actions": "^4.0.0",
    "@storybook/react-native": "^4.0.0",
    "@types/chroma-js": "^1.0.0",
    "@types/fluent-ffmpeg": "^2.0.0",
    "@types/gm": "^1.0.0",
    "@types/jest": "^24.0.0",
    "@types/ramda": "^0.26.0",
    "@types/react-native": "^0.57.0",
    "@types/react-test-renderer": "^16.0.0",
    "@types/semver": "^6.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.0.0",
    "babel-preset-expo": "^5.0.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^5.0.0",
    "eslint-config-universe": "^1.0.0",
    "expo-cli": "^2.0.0",
    "gm": "^1.0.0",
    "husky": "^1.0.0",
    "jest": "^24.0.0",
    "jest-expo": "^33.0.0",
    "lint-staged": "^8.0.0",
    "npm-run-all": "^4.0.0",
    "prettier": "^1.0.0",
    "react-devtools": "^3.0.0",
    "react-dom": "16.8.6",
    "react-test-renderer": "^16.5.0",
    "rimraf": "^2.0.0",
    "semver": "^6.0.0",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.0",
    "tslint": "^5.0.0",
    "tslint-config-prettier": "^1.0.0",
    "typescript": "^3.0.0"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{{packages,scripts,src,storybook}/**/*,*}.{ts,tsx,js,jsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
