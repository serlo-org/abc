{
  "name": "serlo-abc",
  "version": "0.0.0-development",
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "repository": "serlo-org/serlo-abc",
  "scripts": {
    "commit": "git-cz",
    "build": "exp start",
    "build:android": "exp build:android",
    "build:ios": "exp build:ios",
    "build:status": "exp build:status",
    "deploy:development":
      "npm-run-all --serial exp:config exp:login:travis exp:publish:development",
    "deploy:master":
      "npm-run-all --serial exp:config exp:login:travis exp:publish:master",
    "generate": "npm-run-all generate:*",
    "generate:ids": "node scripts/generate-ids",
    "generate:sounds": "ts-node scripts/generate-sounds",
    "generate:videos": "ts-node scripts/generate-videos",
    "generate:words-images": "ts-node scripts/generate-words-images",
    "generate:words-sounds": "ts-node scripts/generate-words-sounds",
    "postgenerate": "yarn format",
    "tsc": "tsc",
    "exp:config": "ts-node scripts/generate-app-config",
    "exp:login": "exp login",
    "exp:login:travis":
      "yarn exp:login --non-interactive --username $EXP_USERNAME --password $EXP_PASSWORD",
    "exp:publish:master": "exp publish",
    "exp:publish:development": "exp publish --release-channel development",
    "start": "react-native-scripts start",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "devtools": "react-devtools",
    "storybook": "storybook start -p 7007",
    "test": "jest",
    "test:watch": "jest --watch",
    "format":
      "prettier --write \"{{packages,scripts,src,storybook}/**/*,*}.{ts,tsx,js,jsx,json,md}\"",
    "lint:js": "eslint \"{{packages,scripts,src,storybook}/**/*,*}.{js,jsx}\"",
    "lint:ts": "tslint \"{{packages,scripts,src,storybook}/**/*,*}.{ts,tsx}\"",
    "lint":
      "npm-run-all --print-name --continue-on-error --sequential lint:* tsc",
    "precommit": "lint-staged",
    "ncu": "ncu -x react"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "chroma-js": "^1.3.6",
    "expo": "^27.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "ramda": "^0.25.0",
    "react": "16.3.1",
    "react-native":
      "https://github.com/expo/react-native/archive/sdk-27.0.0.tar.gz",
    "react-native-swipe-gestures": "^1.0.2",
    "react-router-native": "^4.2.0",
    "sentry-expo": "^1.7.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.2",
    "@storybook/react-native": "^3.4.2",
    "@types/chroma-js": "^1.3.4",
    "@types/fluent-ffmpeg": "^2.1.6",
    "@types/gm": "^1.17.33",
    "@types/jest": "^22.2.3",
    "@types/ramda": "^0.25.24",
    "@types/react-native": "^0.55.8",
    "@types/react-test-renderer": "^16.0.1",
    "@types/storybook__addon-actions": "^3.0.3",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-universe": "^1.0.7",
    "exp": "^53.1.0",
    "gm": "^1.23.1",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-expo-ts": "^22.0.1",
    "lint-staged": "^7.0.5",
    "npm-check-updates": "^2.14.2",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.12.1",
    "react-devtools": "^3.2.1",
    "react-dom": "16.3.1",
    "react-native-scripts": "^1.13.2",
    "react-native-typescript-transformer": "^1.2.5",
    "react-test-renderer": "^16.3.2",
    "ts-node": "^6.0.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^2.8.3"
  },
  "private": true,
  "lint-staged": {
    "{{packages,scripts,src,storybook}/**/*,*}.{ts,tsx,js,jsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>/__mocks__/fileMock.js"
    },
    "preset": "jest-expo-ts",
    "setupFiles": ["<rootDir>/.enzyme.js"],
    "transformIgnorePatterns": []
  }
}
