{
  "extends": [
    "eslint-config-ory-am",
    "eslint-config-ory-am-react"
  ],
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "browser": true
  },
  "rules": {
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "no-invalid-this": ["off"],
    "react/prop-types": ["off"]
  },
  "matrix": {
    "no-console": ["error", { "allow": ["warn", "error"] }]
  }
}
