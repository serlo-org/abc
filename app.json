{
  "expo": {
    "name": "Serlo ABC",
    "slug": "serlo-abc",
    "privacy": "public",
    "sdkVersion": "28.0.0",
    "version": "1.1.0",
    "icon": "./appIcon.png",
    "packagerOpts": {
      "sourceExts": ["ts", "tsx"],
      "transformer":
        "node_modules/react-native-typescript-transformer/index.js",
      "nonPersistent": true
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "android": {
      "package": "org.serlo.abc",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "READ_INTERNAL_STORAGE",
        "READ_PHONE_STATE",
        "RECORD_AUDIO",
        "WRITE_EXTERNAL_STORAGE",
        "com.android.launcher.permission.INSTALL_SHORTCUT",
        "com.htc.launcher.permission.READ_SETTINGS",
        "om.htc.launcher.permission.UPDATE_SHORTCUT"
      ],
      "versionCode": 4
    },
    "assetBundlePatterns": [
      "src/assets/images/*",
      "src/assets/sounds/*",
      "src/assets/words/images/*",
      "src/assets/words/sounds/*"
    ]
  }
}
